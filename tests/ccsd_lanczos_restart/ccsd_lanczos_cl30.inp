system
   name: H2O He
   charge: 0
end system

do
  excited state
end do

memory
   available: 8
end memory

solver cholesky
   threshold: 1.0d-12
end solver cholesky

solver scf
   algorithm:          scf-diis
   energy threshold:   1.0d-12
   gradient threshold: 1.0d-12
end solver scf

method
   hf
   ccsd
end method

solver cc gs
   omega threshold:  1.0d-12
   energy threshold: 1.0d-12
end solver cc gs

solver cc multipliers
   algorithm: diis
   threshold: 1.0d-12
end solver cc multipliers

solver cc es
   algorithm:          asymmetric lanczos
   chain length:       30
end solver cc es

geometry
basis: cc-pVDZ
 H          0.86681        0.60144        5.00000
 H         -0.86681        0.60144        5.00000
 O          0.00000       -0.07579        5.00000
 He         0.10000       -0.02000        7.53000
end geometry
