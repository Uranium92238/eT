system
   name: hypofluorous_acid
end system

do
  ground state
  excited state
end do

memory
   available: 8
end memory

method
   hf
   cc2
end method

! Solver settings:

solver hf
   algorithm: scf-diis
   gradient threshold: 1.0d-11
   energy threshold: 1.0d-11
end solver hf

solver cc gs
   omega threshold: 1.0d-11
   energy threshold: 1.0d-11
end solver cc gs

solver cholesky
   threshold: 1.0d-11
end solver cholesky

solver cc es
   algorithm:          diis A inverse
   singlet states:     4
   residual threshold: 1.0d-11
   energy threshold:   1.0d-11
   left eigenvectors
end solver cc es

geometry
basis: cc-pVDZ
 H          0.86681        0.60144        5.00000
 H         -0.86681        0.60144        5.00000
 O          0.00000       -0.07579        5.00000
 He         0.10000       -0.02000        7.53000
end geometry
