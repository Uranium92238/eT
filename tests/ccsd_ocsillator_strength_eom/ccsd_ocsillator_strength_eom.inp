system
   name: H2O-He
   charge: 0
end system

do
   fop
end do

method
   hf
   ccsd
end method

cc fop
   eom
   dipole length
end cc fop

solver hf
   gradient threshold: 1.0d-12
end solver hf

solver cholesky
   threshold: 1.0d-12
end solver cholesky

solver cc gs
   omega threshold: 1.0d-12
end solver cc gs

solver cc multipliers
   threshold: 1.0d-12
end solver cc multipliers

solver cc es
   residual threshold: 1.0d-12
   singlet states: 1
end solver cc es

geometry
basis: cc-pVDZ
 H          0.86681        0.60144        5.00000
 F         -0.86681        0.60144        5.00000
 O          0.00000       -0.07579        5.00000
 He         0.1000         0.1000         7.5000
end geometry
