system
   name: H2O-He
   charge: 0
end system

do
   excited state
end do

memory
   available: 8
end memory

solver cholesky
   threshold: 1.0d-12
end solver cholesky

solver scf
   algorithm:          scf-diis
   energy threshold:   1.0d-10
   gradient threshold: 1.0d-10
end solver scf

method
   hf
   mlccsd
end method

solver cc gs
   omega threshold:  1.0d-10
   energy threshold: 1.0d-10
end solver cc gs

solver cc es
   algorithm:          davidson
   singlet states:     2
   residual threshold: 1.0d-11
   energy threshold:   1.0d-11
   right eigenvectors
end solver cc es


mlcc
   levels: cc2, ccsd
   ccsd orbitals: cnto
   cnto occupied ccsd: 2
   cnto states: {1,2}
end mlcc

geometry
basis: cc-pVDZ
 H          0.86681        0.60144        5.00000
 H         -0.86681        0.60144        5.00000
 O          0.00000       -0.07579        5.00000
 He         0.10000       -0.02000        7.53000
end geometry
