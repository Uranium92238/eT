system
   name: hypofluorous_acid
end system

do
  ground state
  zop
end do

cc zop
   dipole
end cc zop

memory
   available: 8
end memory

method
   hf
   ccsd
end method

! Solver settings:

solver scf
   algorithm: scf-diis
   gradient threshold: 1.0d-11
   energy threshold: 1.0d-11
end solver scf

solver cc gs
   omega threshold: 1.0d-11
   energy threshold: 1.0d-11
end solver cc gs

solver cc multipliers
   threshold: 1.0d-11
end solver cc multipliers

solver cholesky
   threshold: 1.0d-11
end solver cholesky

! Geometry:

geometry
basis: cc-pVDZ
O   0.00000  0.00000  0.00000
H   1.0805  0.00000  0.00000
F   -.03298340967570528434  0.00000  1.34959701195807505796
