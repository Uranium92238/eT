system
   name: H2OHe
   charge: 0
end system

method
   hf
   ccsd
end method

memory
   available: 8
end memory

solver cholesky
   threshold: 1.0d-12
end solver cholesky

solver hf
  algorithm:          scf-diis
  energy threshold:   1.0d-10
  gradient threshold: 1.0d-10
end solver hf

solver cc gs
   omega threshold:  1.0d-10
   energy threshold: 1.0d-10
end solver cc gs

do
   ground state
   multipliers
end do

solver cc multipliers
    threshold: 1.0d-10
    algorithm: davidson
end solver cc multipliers

geometry
basis: cc-pVDZ
 H          0.86681        0.60144        5.00000
 H         -0.86681        0.60144        5.00000
 O          0.00000       -0.07579        5.00000
 He         0.10000       -0.02000        7.53000
end geometry
