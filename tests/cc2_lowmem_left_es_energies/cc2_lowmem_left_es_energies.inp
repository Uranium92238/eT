system
   name: H2O-He
   charge: 0
end system

do
   ground state
   excited state
end do

memory
   available: 8
end memory

solver cholesky
   threshold: 1.0d-12
end solver cholesky

solver hf
   algorithm:          scf-diis
   energy threshold:   1.0d-11
   gradient threshold: 1.0d-11
end solver hf

method
   hf
   lowmem-cc2
end method

solver cc gs
   omega threshold:  1.0d-11
   energy threshold: 1.0d-11
end solver cc gs

solver cc es
   algorithm:          diis
   singlet states:     4
   residual threshold: 1.0d-11
   energy threshold:   1.0d-11
   left eigenvectors
end solver cc es

geometry
