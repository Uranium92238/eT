system
   name: H2O-He
   charge: 0
end system
 
print 
   output print level: debug 
end print

method
   hf
   cc2
end method

integrals
   cholesky storage: disk
   eri storage:      none 
end integrals

memory
   available: 8
end memory

solver cholesky
   threshold: 1.0d-12
end solver cholesky

solver scf
   algorithm:          scf-diis
   energy threshold:   1.0d-10
   gradient threshold: 1.0d-10
end solver scf

solver cc gs
   omega threshold:  1.0d-10
   energy threshold: 1.0d-10
end solver cc gs

do
   ground state
end do

geometry
basis: cc-pVDZ
 H          0.86681        0.60144        5.00000
 H         -0.86681        0.60144        5.00000
 O          0.00000       -0.07579        5.00000
 He         0.10000       -0.02000        7.53000
end geometry
