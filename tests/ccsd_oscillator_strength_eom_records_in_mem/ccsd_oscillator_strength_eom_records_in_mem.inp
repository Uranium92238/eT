system
   name: HOF-He
   charge: 0
end system
 
print 
   output print level: debug 
end print

do
   fop
end do

method
   hf
   ccsd
end method

cc fop
   eom
   dipole length
   transition moments
end cc fop

solver scf
   storage: memory
   gradient threshold: 1.0d-11
end solver scf

solver cholesky
   threshold: 1.0d-11
end solver cholesky

solver cc gs
   storage: memory
   omega threshold: 1.0d-11
end solver cc gs

solver cc multipliers
   storage: memory
   threshold: 1.0d-11
end solver cc multipliers

solver cc es
   storage: memory
   residual threshold: 1.0d-11
   singlet states: 1
end solver cc es

geometry
basis: cc-pVDZ
 H          0.86681        0.60144        5.00000
 F         -0.86681        0.60144        5.00000
 O          0.00000       -0.07579        5.00000
 He         0.1000         0.1000         7.5000
end geometry
