system
   name: HFO He
   charge: 0
end system

do
   excited state
end do

memory
   available: 16
end memory

solver cholesky
   threshold: 1.0d-12
end solver cholesky

solver scf
   algorithm:          scf-diis
   energy threshold:   1.0d-11
   gradient threshold: 1.0d-11
end solver scf

method
   hf
   ccsd
end method

solver cc gs
   omega threshold:   1.0d-11
   energy threshold:  1.0d-11
end solver cc gs

solver cc multipliers
   threshold: 1.0d-11
end solver cc multipliers

solver cc es
   algorithm:              asymmetric lanczos
   chain length:           30
   lanczos normalization:  symmetric
end solver cc es

geometry
